{
  "expo": {
    "name": "VISHWACHETANA VIDYANIKETANA",
    "slug": "vishwachetana_vidyaniketana",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/schoolIcon.png",
    "scheme": "vv.mobile",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/schoolIcon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.SchoolManagementSystem",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "INTERNET"
      ],
      "usesClearTextTraffic": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "vv.mobile",
              "host": "redirect"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "androidStatusBar": {
        "backgroundColor": "#ac1d1dff",
        "barStyle": "light-content",
        "translucent": false
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-av",
      "expo-audio",
      "expo-asset",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "964f876d-7f93-4f62-84b3-f2a7ffd0bf9c"
      }
    },
    "owner": "space-energy-systems"
  }
}
